{
  "publisher": "seemr",
  "name": "remind-water-helper",
  "displayName": "remind-water-helper",
  "version": "0.0.2",
  "description": "记得喝水",
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "remind-water-helper.water",
        "title": "more water"
