    "lint": "eslint src --ext ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/glob": "^8.1.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "typescript": "^4.9.5"
  }
}
